{
  "source": "docs",
  "destination": "public",
  "plugins": {
    "metalsmith-collections": {},
    "metalsmith-markdown": true,
    "metalsmith-layouts": {
      "engine": "jade",
      "directory": "docs/layout"
    },
    "metalsmith-ignore": [
      "**/.DS_Store",
      "layout/**",
      "css/**"
    ]
  }
}
