  "devDependencies": {
    "babel-plugin-transform-es2015-modules-umd": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "jade": "^1.11.0",
    "metalsmith": "^2.1.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-jade": "^2.0.2",
    "metalsmith-layouts": "^1.4.2",
    "metalsmith-markdown": "^0.2.1",
    "postcss-cli": "^2.3.2",
    "postcss-color-function": "^2.0.0",
