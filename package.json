  },
  "homepage": "https://github.com/leroy-merlin-br/garden#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-umd": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "cssnano": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "isparta": "^4.0.0",
    "istanbul": "gotwarlost/istanbul.git#source-map",
    "karma": "^0.13.16",
